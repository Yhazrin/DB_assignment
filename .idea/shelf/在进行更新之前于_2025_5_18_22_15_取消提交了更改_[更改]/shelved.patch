<+>/* variables-dark.css\r\n * 完整暗色主题变量定义\r\n * 单文件覆盖所有配色与渐变至暗色风格\r\n */\r\nhtml.dark-mode,\r\nbody.dark-theme {\r\n    /* ——— 通用变量 (与 light 一致) ——— */\r\n    /* Spacing */\r\n    --spacing-xs: 0.25rem;\r\n    --spacing-sm: 0.5rem;\r\n    --spacing-md: 1rem;\r\n    --spacing-lg: 2rem;\r\n    --spacing-xl: 4rem;\r\n\r\n    /* Font sizes */\r\n    --font-size-sm: 0.875rem;\r\n    --font-size-md: 1rem;\r\n    --font-size-lg: 1.25rem;\r\n    --font-size-xl: 2rem;\r\n    --font-size-xxl: 3rem;\r\n    --font-size-hero: 5rem;\r\n\r\n    /* Border radius */\r\n    --radius-sm: 4px;\r\n    --radius-md: 8px;\r\n    --radius-lg: 16px;\r\n\r\n    /* Z-index tiers */\r\n    --z-header: 1000;\r\n    --z-modal: 2000;\r\n\r\n    /* Transition durations */\r\n    --transition-fast: 0.2s;\r\n    --transition-normal: 0.5s;\r\n    --transition-slow: 1s;\r\n\r\n    /* Gradient animation settings */\r\n    --gradient-duration: 8s;\r\n    --gradient-easing: ease-in-out;\r\n    --gradient-iteration: infinite;\r\n\r\n    /* Section minimum heights */\r\n    --section-height-hero: 100vh;\r\n    --section-height-default: 60vh;\r\n    --section-height-compact: auto;\r\n\r\n    --header-bg: rgb(255, 255, 255);  /* 夜间导航背景 */\r\n    --header-text: #000000;/* 夜间导航文字 */\r\n    --headerhome-bg: rgba(0, 0, 0, 0.6);  /* 夜间导航背景 */\r\n    --headerhome-text: #ffffff;/* 夜间导航文字 */\r\n\r\n    /* ——— 暗色主题配色 ——— */\r\n    /* 背景色 */\r\n    --bg-primary: #000000;\r\n    --bg-inverse: #ffffff;\r\n    --bg-banner: linear-gradient(135deg, #1a1a1a, #2d2d2d);\r\n    /* 文本色 */\r\n    --text-primary: #ffffff;\r\n    --text-inverse: #000000;\r\n    --text-hero: #ffffff;\r\n    --text-color: var(--text-primary);\r\n\r\n    /* 主色、次色 */\r\n    --primary-color:   #3a3a60;\r\n    --secondary-color: #4b4b80;\r\n\r\n    --card-bg: #000000;\r\n    --divider: #c2574d;\r\n    --card-border : rgba(255, 255, 255, 0.55);\r\n\r\n    --btn-primary: #3c3c3c;\r\n    --btn-border: #ffc5c2;\r\n    --btn-border-hover: #7089ff;\r\n\r\n    /* 区块渐变背景 */\r\n    --gradient-hero:     linear-gradient(135deg, #000000, #000000);\r\n    --gradient-features: linear-gradient(135deg, #0A2E38, #1A4A4A);\r\n    --gradient-overview: linear-gradient(135deg, #2F1640, #45306A);\r\n    --gradient-compare:  linear-gradient(135deg, #1F2D70, #243B7A);\r\n    --gradient-forum:    linear-gradient(135deg, #2D3345, #4A5A76);\r\n    --gradient-profile:  linear-gradient(135deg, #7b5956, #451e6f);\r\n    --gradient-cta:      linear-gradient(135deg, #200E40, #3A225B);\r\n\r\n    /* ——— 流光按钮专属变量 ——— */\r\n    --border-width: 0.25em;\r\n    --border-radius: 1em;\r\n    --border-color: #ffd0d0;\r\n    --border-blur: 0.75em;\r\n    --transition-speed: 0.5s;\r\n}
