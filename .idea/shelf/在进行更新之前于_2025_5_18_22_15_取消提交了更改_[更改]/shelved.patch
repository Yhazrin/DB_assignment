========= */\r\n@media (max-width: 768px) {\r\n    .main-container {\r\n        flex-direction: column;\r\n    }\r\n    .profile-sidebar,\r\n    .profile-content {\r\n        width: 100%;\r\n    }\r\n    .profile-sidebar {\r\n        margin-bottom: var(--spacing-lg);\r\n    }\r\n}\r\n
