<+>@import \"../../style/variables.css\";\r\n@import \"../../utilities.css\";\r\n@import \"../../components/header.css\";\r\n@import \"../../components/layout.css\";\r\n@import \"../../components/cards.css\";\r\n\r\nhtml, body {\r\n    height: 100%;\r\n    margin: 0;\r\n    padding: 0;\r\n    overflow: hidden;\r\n}\r\n\r\n.main-container {\r\n    display: flex;\r\n    position: absolute;\r\n    gap: 1rem;\r\n    top: 0;\r\n    left: 0;\r\n    right: 0;\r\n    bottom: 0;\r\n    box-sizing: border-box;\r\n}\r\n/* ========== Sidebar ========== */\r\n.profile-sidebar {\r\n    width: 10%;\r\n    padding: var(--font-size-md);\r\n\r\n}\r\n\r\n.profile-sidebar,\r\n.profile-main {\r\n    background: var(--card-bg);\r\n    backdrop-filter: blur(8px);\r\n    -webkit-backdrop-filter: blur(8px);\r\n    border-radius: var(--radius-md);\r\n    box-shadow: 0 8px 24px rgba(0, 0, 0, 0.2);\r\n    display: flex;\r\n    border: 3px solid var(--card-border);\r\n    flex-direction: column;\r\n    justify-content: flex-start;\r\n    transition: transform var(--transition-fast), box-shadow var(--transition-fast);\r\n}\r\n\r\n.profile-sidebar .sidebar-section {\r\n    background: var(--gradient-profile);\r\n    backdrop-filter: blur(20px);\r\n    -webkit-backdrop-filter: blur(8px);\r\n    border-radius: var(--radius-md);\r\n    padding: var(--spacing-sm) 0 ;\r\n    margin-bottom: var(--spacing-md);\r\n    display: flex;\r\n    flex-direction: column;\r\n    justify-content: flex-start;\r\n    transition: transform var(--transition-fast),\r\n    border-color var(--transition-fast); /* 新增边框过渡 */\r\n    background: rgba(255, 255, 255, 0.12);\r\n    border: 4px solid var(--secondary-color);\r\n    font-weight: 800;\r\n    text-align: center;\r\n    white-space: nowrap;\r\n    overflow: hidden;\r\n}\r\n\r\n\r\n/* User Card */\r\n.profile-sidebar .user-card {\r\n    text-align: center;\r\n}\r\n\r\n.profile-sidebar .avatar-wrapper {\r\n    width: var(--spacing-xxl);          /* 6rem (96px) */\r\n    height: var(--spacing-xxl);\r\n    border-radius: 50%;\r\n    overflow: hidden;\r\n}\r\n\r\n.profile-sidebar .avatar-wrapper img {\r\n    width: 100%;\r\n    height: 100%;\r\n    object-fit: cover;\r\n    display: block;\r\n}\r\n\r\n\r\n/* Social Stats */\r\n.profile-sidebar .social-stats {\r\n    display: flex;\r\n    flex-direction: column;\r\n    gap: var(--spacing-md);\r\n}\r\n\r\n.profile-sidebar .social-stats .stat-item {\r\n    font-size: var(--font-size-md);\r\n    color: var(--text-color);\r\n}\r\n\r\n/* Navigation Links */\r\n.profile-sidebar .sidebar-nav .nav-item {\r\n    display: block;\r\n    padding: var(--spacing-sm) var(--spacing-md);\r\n    margin-bottom: var(--spacing-sm);\r\n    font-size: var(--font-size-md);\r\n    color: var(--text-color);\r\n    text-decoration: none;\r\n    border-radius: var(--radius-sm);\r\n    transition: background var(--transition-fast);\r\n}\r\n\r\n.profile-sidebar .sidebar-nav .nav-item:hover,\r\n.profile-sidebar .sidebar-nav .nav-item.active {\r\n    background: var(--primary-color);\r\n    color: var(--text-inverse);\r\n}\r\n\r\n.profile-sidebar .sidebar-nav .danger {\r\n    color: var(--primary-color);\r\n}\r\n\r\n/* ========== Main Content ========== */\r\n.profile-main {\r\n    width: 100%;\r\n}\r\n\r\n.profile-content {\r\n    flex: 1;\r\n    display: flex;\r\n    flex-direction: column;\r\n    padding: var(--spacing-lg);\r\n    box-sizing: border-box;\r\n\r\n    height: 100%;\r\n}\r\n\r\n/* Breadcrumb area (inherited from forum.css) */\r\n/* .content-header { … } */\r\n\r\n/* ========== Sections ========== */\r\n.content-section {\r\n    display: none;\r\n}\r\n\r\n.content-section.active {\r\n    display: block;\r\n}\r\n\r\n/* Section Title */\r\n.section-title {\r\n    font-size: var(--font-size-xl);\r\n    margin-bottom: var(--spacing-md);\r\n    border-bottom: 2px solid var(--divider);\r\n    padding-bottom: var(--spacing-sm);\r\n    color: var(--text-color);\r\n}\r\n\r\n/* Basic Info Grid */\r\n.info-grid