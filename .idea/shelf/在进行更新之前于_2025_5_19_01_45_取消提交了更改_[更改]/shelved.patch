+// login.js
+document.addEventListener('DOMContentLoaded', function() {
+    const form = document.getElementById('loginForm');
+    const errorDiv = document.getElementById('loginError');
+
+    if (!form) return; // 防止找不到form时出错
+
+    form.addEventListener('submit', function(e) {
+        e.preventDefault();
+
+        const username = document.getElementById('username').value.trim();
+        const password = document.getElementById('password').value.trim();
+
+        if (!username || !password) {
+            errorDiv.textContent = "Please enter both username and password.";
+            return;
+        }
+
+        fetch(
+            `${window.location.origin}${window.location.pathname.replace(/\/[^/]*$/, "")}/data?type=login&username=${encodeURIComponent(username)}&password=${encodeURIComponent(password)}`,
+            { method: "POST" }
+        )
+            .then(res => res.json())
+            .then(data => {
+                if (data.result === "success") {
+                    // 登录成功，跳转主页
+                    window.location.href = `${window.location.origin}${window.location.pathname.replace(/\/[^/]*$/, "")}/index.jsp`;
+                } else {
+                    errorDiv.textContent = data.message || "Login failed, please try again.";
+                }
+            })
+            .catch(() => {
+                errorDiv.textContent = "Network error, please try again later.";
+            });
+    });
+});
