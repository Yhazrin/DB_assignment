<+>/* compare.css — 针对 Compare JSP 结构的样式 */\r\n\r\n/* 全局引入：变量、布局、按钮、卡片等 */\r\n@import \"../style/variables.css\";\r\n@import \"../utilities.css\";\r\n@import \"../components/layout.css\";\r\n@import \"../components/banner.css\";\r\n@import \"../components/buttons.css\";\r\n@import \"../components/header.css\";\r\n\r\n/* 顶部横幅 */\r\n.main-container .banner {\r\n    text-align: center;\r\n}\r\n\r\n/* 控件区域 */\r\n.controls {\r\n    display: flex;\r\n    justify-content: space-between;\r\n    align-items: center;\r\n    margin: var(--spacing-lg) 0;\r\n    background: var(--card-bg);\r\n    padding: var(--spacing-md) var(--spacing-lg);\r\n    border-radius: var(--radius-md);\r\n    box-shadow: 0 2px 12px rgba(0, 0, 0, 0.06);\r\n}\r\n.control-group {\r\n    display: flex;\r\n    align-items: center;\r\n    gap: var(--spacing-md);\r\n}\r\n.control-group label {\r\n    font-weight: 600;\r\n    color: var(--text-color);\r\n    white-space: nowrap;\r\n    margin-right: 0.5rem;\r\n}\r\n.controls select {\r\n    padding: var(--spacing-sm) var(--spacing-md);\r\n    border: 1px solid var(--border-color);\r\n    border-radius: var(--radius-sm);\r\n    background: var(--card-bg);\r\n    color: var(--text-color);\r\n    transition: border-color var(--transition-normal);\r\n}\r\n.controls select:hover {\r\n    border-color: var(--primary-color);\r\n}\r\n\r\n/* Reset / Compare 按钮 */\r\n#reset-btn,\r\n#compare-btn {\r\n    margin-left: var(--spacing-md);\r\n}\r\n\r\n/* 选择器（动态生成插入的按钮组）*/\r\n.selectors {\r\n    display: flex;\r\n    flex-wrap: wrap;\r\n    gap: var(--spacing-md);\r\n    margin-bottom: var(--spacing-lg);\r\n}\r\n\r\n/* 对比结果表格 */\r\n.compare-table {\r\n    width: 100%;\r\n    border-collapse: collapse;\r\n    margin-bottom: var(--spacing-lg);\r\n}\r\n.compare-table th,\r\n.compare-table td {\r\n    padding: var(--spacing-sm) var(--spacing-md);\r\n    border: 1px solid var(--divider);\r\n    text-align: center;\r\n}\r\n.compare-table th {\r\n    background: var(--section-highlight);\r\n    color: var(--text-color);\r\n    font-weight: 600;\r\n}\r\n\r\n/* 无数据提示 */\r\n.message {\r\n    text-align: center;\r\n    padding: var(--spacing-lg);\r\n    color: var(--text-secondary);\r\n}\r\n\r\n/* 操作条 */\r\n.action-bar {\r\n    display: flex;\r\n    justify-content: flex-end;\r\n    gap: var(--spacing-md);\r\n    margin-top: var(--spacing-lg);\r\n}\r\n\r\n/* 响应式：窄屏下表格折叠为卡片式展示 */\r\n@media (max-width: 768px) {\r\n    .compare-table,\r\n    .compare-table th,\r\n    .compare-table td {\r\n        display: block;\r\n        width: 100%;\r\n    }\r\n    .compare-table th {\r\n        text-align: left;\r\n    }\r\n    .compare-table td {\r\n        position: relative;\r\n        padding-left: 50%;\r\n        text-align: right;\r\n    }\r\n    .compare-table td::before {\r\n        content: attr(data-label) \": \";\r\n        position: absolute;\r\n        left: var(--spacing-sm);\r\n        font-weight: 600;\r\n    }\r\n}\r\n\r\n/* —— 比较页面机型选择区域 —— */\r\n.selectors {\r\n    display: flex;\r\n    flex-wrap: wrap;\r\n    gap: var(--spacing-md);\r\n    margin: var(--spacing-lg) 0;\r\n    justify-content: flex-start;  /* 横向并排 */\r\n}\r\n\r\n.model-selector {\r\n    position: relative;\r\n    flex: 1 1 200px;              /* 最小 200px，剩余空间平分 */\r\n    max-width: 300px;             /* 可选：限制最大宽度 */\r\n}\r\n\r\n.model-input {\r\n    width: 100%;\r\n    padding: var(--spacing-sm) var(--spacing-md);\r\n    border: 1px solid var(--divider);\r\n    border-radius: var(--radius-sm);\r\n    background: var(--card-bg);\r\n    color: var(--text-color);\r\n    font-size: var(--font-size-md);\r\n    box-sizing: border-box;\r\n    transition: border-color var(--transition-normal);\r\n}\r\n\r\n.model-input:focus {\r\n    outline: none;\r\n    border-color: var(--primary-color);\r\n    box-shadow: 0 0 0 3px rgba( var(--bg-inverse), 0.15 );\r\n}\r\n\r\n.search-results {\r\n    position: absolute;\r\n    top: calc(100% + var(--spacing-xs));\r\n    left: 0;\r\n    right: 0;\r\n    background: var(--card-bg);\r\n    border: 1px solid var(--divider);\r\n    border-radius: var(--radius-sm);\r\n    max-height: 200px;\r\n    overflow-y: auto;\r\n    box-shadow: 0 4px 8px rgba(0, 0, 0, 0.1);\r\n    display: none;\r\n    z-index: 50;\r\n}\r\n\r\n.search-results.show {\r\n    display: block;\r\n}\r\n\r\n.search-result-item {\r\n    padding: var(--spacing-sm) var(--spacing-md);\r\n    cursor: pointer;\r\n    transition: background var(--transition-fast);\r\n}\r\n\r\n.search-result-item:hover {\r\n    background: rgba( var(--bg-inverse), 0.1 );\r\n}\r\n\r\n.selected-model {\r\n    display: flex;\r\n    align-items: center;\r\n    justify-content: space-between;\r\n    background: var(--card-bg);\r\n    padding: var(--spacing-sm) var(--spacing-md);\r\n    border: 1px solid var(--divider);\r\n    border-radius: var(--radius-sm);\r\n    margin-top: var(--spacing-xs);\r\n}\r\n\r\n.selected-model span {\r\n    font-size: var(--font-size-sm);\r\n}\r\n\r\n.selected-model .remove-btn {\r\n    margin-left: var(--spacing-sm);\r\n    cursor: pointer;\r\n    font-weight: bold;\r\n    line-height: 1;\r\n}\r\n\r\n
